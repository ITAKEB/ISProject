<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pk2app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Pk2App">
        <activity
            android:name=".AccountView"
            android:exported="false"
            />
        <activity
            android:name=".AItems"
            android:exported="false"
            android:theme="@style/Theme.Pk2App.NoActionBar"></activity>
        <activity
            android:name=".AaccountsPayed"
            android:exported="false"
            android:theme="@style/Theme.Pk2App.NoActionBar" />
        <activity
            android:name=".Adelete"
            android:exported="false"
            android:theme="@style/Theme.Pk2App.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Pk2App.NoActionBar"
            android:windowSoftInputMode="adjustPan">
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>